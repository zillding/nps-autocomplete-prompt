{
  "name": "nps-autocomplete-prompt",
  "version": "1.1.0",
  "description": "Autocomplete prompt for nps scripts",
  "repository": "github:zillding/nps-autocomplete-prompt",
  "engines": {
    "node": ">=8"
  },
  "bin": {
    "nap": "./cli.js"
  },
  "scripts": {
    "start": "nps",
    "test": "nps lint test"
  },
  "keywords": [
    "nps",
    "autocomplete",
    "prompt"
  ],
  "author": "Zill Ding",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "execa": "^2.0.1",
    "flat": "^4.1.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "js-yaml": "^3.13.1"
  },
  "devDependencies": {
    "eslint": "^6.0.0",
    "husky": "^2.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "nps": "^5.9.3",
    "prettier": "^1.16.4"
  }
}
